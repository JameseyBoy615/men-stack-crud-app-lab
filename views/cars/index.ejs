<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Favorite Cars</title>
  </head>
  <body>
    <h1>Your Cars</h1>
    <ul>
      <% cars.forEach(car => { %>
      <li>
        <a href="/cars/<%= car.id %>"> <%= car.make %> <%= car.model %> </a>
      </li>
      <% }); %>
    </ul>
    <a href="/cars/new">Add New Car</a>
  </body>
</html>
